<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:entityViewModel="clr-namespace:Imagio.Helpdesk.ViewModel.Entity"
                    xmlns:entityView="clr-namespace:Imagio.Helpdesk.View.Entity">
    
    <DataTemplate DataType="{x:Type entityViewModel:SoftwareViewModel}">
        <entityView:SoftwareView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type entityViewModel:AccountViewModel}">
        <entityView:AccountView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type entityViewModel:EmployeeViewModel}">
        <entityView:EmployeeView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type entityViewModel:FirmViewModel}">
        <entityView:SimpleView />
    </DataTemplate>
    
</ResourceDictionary>